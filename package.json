{
	"name": "@ctx-core/regex",
	"version": "8.1.30",
	"description": "ctx-core regex",
	"keywords": [
		"ctx-core",
		"regex"
	],
	"homepage": "https://github.com/ctx-core/regex#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/regex/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/regex.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./re_",
		"./regex_indexOf",
		"./regex_lastIndexOf"
	],
	"types": "./index.d.ts",
	"svelte": "./index.js",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}
